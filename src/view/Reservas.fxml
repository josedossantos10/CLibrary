<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReservasController">
   <children>
      <Button layoutX="288.0" layoutY="380.0" mnemonicParsing="false" onAction="#efetuarReserva" text="Efetuar">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Button>
      <Label layoutX="139.0" layoutY="14.0" text="Reservas Disponíveis para Empréstimo">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TableView fx:id="tabelaReservasUsuario" layoutX="14.0" layoutY="63.0" prefHeight="308.0" prefWidth="676.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="colunaIdReserva" maxWidth="30.0" minWidth="25.0" prefWidth="30.0" resizable="false" text="ID" />
            <TableColumn fx:id="colunaNomeReserva" editable="false" minWidth="150.0" prefWidth="200.0" text="Nome" />
            <TableColumn fx:id="colunaDataReserva" maxWidth="150.0" minWidth="70.0" prefWidth="74.0" resizable="false" text="Data" />
            <TableColumn fx:id="colunaInicioReserva" maxWidth="150.0" minWidth="70.0" prefWidth="74.0" resizable="false" text="Data Inicio" />
            <TableColumn fx:id="colunaLivroReserva" editable="false" minWidth="180.0" prefWidth="266.0" text="Livro" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
